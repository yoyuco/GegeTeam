          "action": "commit_changes"
        }
      ],
      "successRate": 0.89
    },
    "bug_fixing": {
      "name": "Bug Fixing Workflow",
      "description": "Workflow for debugging and fixing issues",
      "steps": [
        {
          "tool": "vue_dev_server",
          "action": "start",
          "parameters": {
            "port": 83
          }
        },
        {
          "tool": "implementation",
          "action": "debug_and_fix"
        },
        {
          "tool": "implementation",
          "action": "test_fix"
        }
      ],
      "successRate": 0.94
    },
    "code_quality_assurance": {
      "name": "Code Quality Assurance Workflow",
      "description": "Complete code quality check with Serena AI",
      "steps": [
        {
          "tool": "implementation",
          "action": "generate_code"
        },
        {
          "tool": "serena",
          "action": "analyze_code_quality",
          "parameters": {
            "depth": "comprehensive",
            "check_best_practices": true,
            "focus": ["performance", "maintainability", "security"]
          }
        },
        {
          "tool": "serena",
          "action": "suggest_improvements",
          "parameters": {
            "priority": "high",
            "actionable": true
          }
        },
        {
          "tool": "context7",
          "action": "save_analysis",
          "parameters": {
            "key": "code_quality_analysis",
            "persist": true,
            "tags": ["quality", "analysis", "serena"]
          }
        }
      ],
      "successRate": 0.95
    },
    "session_management": {
      "name": "Session Management Workflow",
      "description": "Manage session context and persistence",
      "steps": [
        {
          "tool": "context7",
          "action": "load_session_context",
          "parameters": {
            "session_id": "current",
            "include_history": true
          }
        },
        {
          "tool": "implementation",
          "action": "execute_task"
        },
        {
          "tool": "context7",
          "action": "save_session_progress",
          "parameters": {
            "include_context": true,
            "persist": true,
            "update_timestamp": true
          }
        }
      ],
      "successRate": 0.98
    },
    "ai_assisted_development": {
      "name": "AI-Assisted Development Workflow",
      "description": "Complete development workflow with AI assistance",
      "steps": [
        {
          "tool": "context7",
          "action": "load_project_context"
        },
        {
          "tool": "implementation",
          "action": "create_initial_code"
        },
        {
          "tool": "serena",
          "action": "analyze_and_improve",
          "parameters": {
            "comprehensive": true,
            "suggestions": true
          }
        },
        {
          "tool": "figma_compare",
          "action": "validate_design",
          "condition": "design_available"
        },
        {
          "tool": "serena",
          "action": "generate_documentation",
          "parameters": {
            "format": "markdown",
            "include_examples": true
          }
        },
        {
          "tool": "context7",
          "action": "save_complete_session",
          "parameters": {
            "include_artifacts": true,
            "persist": true
          }
        }
      ],
      "successRate": 0.9
    }
