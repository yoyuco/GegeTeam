  "workflows": {
    "design_implementation": {
      "name": "Design Implementation Workflow",
      "description": "Complete workflow for implementing design components",
      "steps": [
        {
          "tool": "vue_dev_server",
          "action": "start",
          "parameters": {
            "port": 83
          }
        },
        {
          "tool": "implementation",
          "action": "create_component"
